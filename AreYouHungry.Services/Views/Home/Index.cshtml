<script>
    function go() {
        $.ajax({
            url: "http://localhost:2715/Token",
            type: "POST",
            data: "grant_type=password&username=admin&password=123456",
            success: function (data) {
                console.log(data);
                console.log("yes");
                sessionStorage["accessToken"] = data.access_token;
                location.href = "/home";
            }
        });
    }

    function reg() {
        var model = {
            "UserName": "admin",
            "Password": "123456",
            "ConfirmPassword": "123456",
            "Email": "sample string 4"
        };

        $.ajax({
            url: "http://localhost:2715/api/account/register",
            type: "POST",
            data: model,
            success: function (data) {
                console.log(data);
                console.log("yes");
                sessionStorage["accessToken"] = data.access_token;
                location.href = "/home";
            }
        });
    }
</script>
